<%- model_class = LikeSpecialPartUserUser -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:special_part_id) %></th>
      <th><%= model_class.human_attribute_name(:church_id) %></th>
      <th><%= model_class.human_attribute_name(:user_id) %></th>
      <th><%= model_class.human_attribute_name(:like) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @like_special_part_user_users.each do |like_special_part_user_user| %>
      <tr>
        <td><%= link_to like_special_part_user_user.id, like_special_part_user_user_path(like_special_part_user_user) %></td>
        <td><%= like_special_part_user_user.special_part_id %></td>
        <td><%= like_special_part_user_user.church_id %></td>
        <td><%= like_special_part_user_user.user_id %></td>
        <td><%= like_special_part_user_user.like %></td>
        <td><%=l like_special_part_user_user.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_like_special_part_user_user_path(like_special_part_user_user), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      like_special_part_user_user_path(like_special_part_user_user),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_like_special_part_user_user_path,
            :class => 'btn btn-primary' %>
